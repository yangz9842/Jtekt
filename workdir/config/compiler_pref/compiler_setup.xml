<?xml version="1.0" encoding="UTF-8"?>
<EpexCompilerSetup replaceable="true">
	<!-- Variables: INSTALL_PATH -->
	<compiler name="GCC" additionalInfo="GCC64 Compiler (C)" executable="gcc" location="/usr/bin" bitWidth="64">
		<outputObjectFileOption option="-o" spaceKeyValue="true" />
		<objectGenerationOption option="-c" spaceKeyValue="true" />
		<defaultIncludePaths>
			<!--  Call "gcc -xc -E -v -" to find out the default include paths -->
			<item value="/usr/lib/gcc/x86_64-linux-gnu/9/include" />
			<item value="/usr/local/include" />
			<item value="/usr/include/x86_64-linux-gnu" />
			<item value="/usr/include" />
		</defaultIncludePaths>
		<defaultCompilerFlags>
			<item value="-fPIC" />
			<item value="-std=gnu90" />
			<item value="-fwrapv" />
		</defaultCompilerFlags>
		<defaultCompilerFlagsC99>
			<item value="-fPIC" />
			<item value="-std=gnu99" />
			<item value="-fwrapv" />
		</defaultCompilerFlagsC99>
		<defaultCompilerFlagsCPP>
			<item value="-fPIC" />
			<item value="-std=gnu++11" />
			<item value="-x c++" />
			<item value="-fwrapv" />
		</defaultCompilerFlagsCPP>
		<defaultDefinesC90>
			<!-- TODO: remove this workaround for _Float128 support as long as code_extraction (1) does  -->
			<!--  does not yet support _Float128 (see MRDEVT-1104). code_extraction2 has support for it. -->
			<pair key="_Float32" value="float" />
			<pair key="_Float32x" value="double" />
			<pair key="_Float64" value="double" />
			<pair key="_Float64x" value="long double" />
			<pair key="_Float128" value="long double" />
		</defaultDefinesC90>
		<defaultDefinesC99>
			<!-- TODO: remove this workaround for _Float128 support as long as code_extraction (1) does  -->
			<!--  does not yet support _Float128 (see MRDEVT-1104). code_extraction2 has support for it. -->
			<pair key="_Float32" value="float" />
			<pair key="_Float32x" value="double" />
			<pair key="_Float64" value="double" />
			<pair key="_Float64x" value="long double" />
			<pair key="_Float128" value="long double" />
		</defaultDefinesC99>
		<includeFolderOption option="-I" spaceKeyValue="false" />
		<macroDefineOption option="-D" spaceKeyValue="false" />
		<environmentVariables>
			<pair key="PATH" value="/usr/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin" />
			<pair key="LIB" value="&#xA;" />
		</environmentVariables>
	</compiler>
	<libtool executable="gcc" location="/usr/bin">
		<outputFileOption option="-o" spaceKeyValue="true" />
	</libtool>
	<linker executable="gcc" executableCPP="g++" location="/usr/bin">
		<defaultLinkerFlags>
			<item value="-L&quot;/usr/lib&quot;" />
		</defaultLinkerFlags>
		<dllOption option="-shared" spaceKeyValue="true" />
		<outputFileOption option="-o" spaceKeyValue="true" />
	</linker>
</EpexCompilerSetup>
